        <ComboBox x:Name="Box1" HorizontalAlignment="Left" Margin="23,71,0,0" VerticalAlignment="Top" Width="120" SelectedIndex="0"/>
        <ComboBox x:Name="Box2" HorizontalAlignment="Left" Margin="391,71,0,0" VerticalAlignment="Top" Width="120" SelectedIndex="10"/>
